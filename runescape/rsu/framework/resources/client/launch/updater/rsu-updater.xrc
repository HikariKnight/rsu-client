<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<resource xmlns="http://www.wxwindows.org/wxxrc" version="2.3.0.1">
	<object class="wxFrame" name="mainwindow">
		<style>wxDEFAULT_FRAME_STYLE|wxTAB_TRAVERSAL</style>
		<size>663,395</size>
		<title>Client Updater</title>
		<centered>1</centered>
		<aui_managed>0</aui_managed>
		<object class="wxPanel" name="mainpanel">
			<style>wxTAB_TRAVERSAL</style>
			<object class="wxGridSizer">
				<rows>0</rows>
				<cols>1</cols>
				<vgap>0</vgap>
				<hgap>0</hgap>
				<object class="sizeritem">
					<option>1</option>
					<flag>wxEXPAND | wxALL</flag>
					<border>5</border>
					<object class="wxNotebook" name="m_notebook1">
						<object class="notebookpage">
							<label>Client Updater</label>
							<selected>0</selected>
							<object class="wxPanel" name="clientpage">
								<style>wxTAB_TRAVERSAL</style>
								<object class="wxFlexGridSizer">
									<rows>2</rows>
									<cols>1</cols>
									<vgap>0</vgap>
									<hgap>0</hgap>
									<growablecols>0</growablecols>
									<growablerows>1</growablerows>
									<object class="sizeritem">
										<option>0</option>
										<flag>wxALIGN_CENTER|wxALL|wxBOTTOM</flag>
										<border>5</border>
										<object class="wxStaticText" name="legalnote">
											<style>wxALIGN_CENTRE</style>
											<label>For this client to work you must be able to download the Official RuneScape Client.\nNOTE: It does not matter which one you choose unless you choose\nto run the client in winemode, in that case you must use the files from the Windows Client</label>
											<wrap>-1</wrap>
										</object>
									</object>
									<object class="sizeritem">
										<option>1</option>
										<flag>wxALL|wxEXPAND</flag>
										<border>5</border>
										<object class="wxStaticBoxSizer">
											<orient>wxVERTICAL</orient>
											<label>What do you want to do?</label>
											<object class="sizeritem">
												<option>1</option>
												<flag>wxEXPAND | wxALL</flag>
												<border>5</border>
												<object class="wxScrolledWindow" name="scrolledlist">
													<style>wxHSCROLL|wxVSCROLL</style>
													<scrollrate>5,5</scrollrate>
												</object>
											</object>
										</object>
									</object>
								</object>
							</object>
						</object>
						<object class="notebookpage">
							<label>Addon Updater</label>
							<selected>0</selected>
							<object class="wxPanel" name="addonpage">
								<style>wxTAB_TRAVERSAL</style>
								<object class="wxFlexGridSizer">
									<rows>2</rows>
									<cols>1</cols>
									<vgap>0</vgap>
									<hgap>0</hgap>
									<growablecols>0</growablecols>
									<growablerows>1</growablerows>
									<object class="sizeritem">
										<option>0</option>
										<flag>wxALIGN_CENTER|wxALL</flag>
										<border>5</border>
										<object class="wxStaticText" name="addonnotice">
											<style>wxALIGN_CENTRE</style>
											<label>All addons that have added their entry to the updater will be listed here.\nNOTE: The launcher must be restarted to detect new addons!\n</label>
											<wrap>-1</wrap>
										</object>
									</object>
									<object class="sizeritem">
										<option>1</option>
										<flag>wxALL|wxEXPAND</flag>
										<border>5</border>
										<object class="wxStaticBoxSizer">
											<orient>wxVERTICAL</orient>
											<label>Which addon do you want to download/update?</label>
											<object class="sizeritem">
												<option>1</option>
												<flag>wxEXPAND | wxALL</flag>
												<border>5</border>
												<object class="wxScrolledWindow" name="addonlist">
													<style>wxHSCROLL|wxVSCROLL</style>
													<scrollrate>5,5</scrollrate>
												</object>
											</object>
										</object>
									</object>
								</object>
							</object>
						</object>
					</object>
				</object>
			</object>
		</object>
		<object class="wxStatusBar" name="m_statusBar1">
			<style>wxST_SIZEGRIP</style>
			<fields>1</fields>
		</object>
	</object>
</resource>
